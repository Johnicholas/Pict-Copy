*** Arithmetic operations ***
 y <> 0 ==> x == x * (x / y) + (x % y)
 x >= 0 && y >> 0 ==> 0 <= (x % y) << y
 x + 0 == x
 x + y == y + x
 x * 0 == 0
 x * 1 == x
 x * y == y * x

*** Bitwise arithmetic operations ***
 (lnot(lnot x)) == x
 (lnot 0) = -1
 (lnot 255) = -256
 (lor x x) == x
 (lor x y) == (lor y x)
 (lor 32 15) = 47
 (lor 31 15) = 31
 (land x x) == x
 (land x y) == (land y x)
 (land 32 15) = 0
 (land 31 15) = 15
 (lxor x x) == 0
 (lxor x 0) == x
 (lxor x y) == (lxor y x)
 (lxor 32 15) = 47
 (lxor 31 15) = 16
 (lor (lnot x) x) == -1
 (land (lnot x)) == 0
